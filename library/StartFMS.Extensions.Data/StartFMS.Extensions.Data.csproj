<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net6.0;net7.0;net8.0</TargetFrameworks>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>
  
  <PropertyGroup>
      <PackageId>StartFMS.Extenstions.Data</PackageId>
      <Title>StartFMS</Title>
      <Version>1.0.0</Version>
      <Authors>John Conte</Authors>
      <Company>-</Company>
      <Product>StartFMS</Product>
      <Description>Models 轉換、字串轉換</Description>
      <Copyright>© StartFMS Corporation.  All rights reserved.</Copyright>
      <PackageLicenseUrl></PackageLicenseUrl>
      <PackageProjectUrl>https://github.com/JontCont/StartFMS</PackageProjectUrl>
      <PackageIconUrl></PackageIconUrl>
      <PackageTags></PackageTags>
      <PackageReleaseNotes></PackageReleaseNotes>
      <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
      <RepositoryUrl>https://github.com/JontCont/StartFMS</RepositoryUrl>
      <RepositoryType>git</RepositoryType>
      <IncludeSymbols>true</IncludeSymbols>
      <IncludeSource>true</IncludeSource>
      <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
  </PropertyGroup>
</Project>
